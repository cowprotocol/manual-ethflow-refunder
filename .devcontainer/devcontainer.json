{
    "name": "Manual Ethflow Refunder",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode"
            ]
        }
    },
    "forwardPorts": [],
    "postCreateCommand": "yarn install --frozen-lockfile",
    "remoteUser": "node",
    "features": {
        "ghcr.io/devcontainers/features/node:1": {
            "version": "18"
        }
    }
}
